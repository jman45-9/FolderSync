{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build with GCC",
        "type": "shell",
        "command": "gcc",
        "args": ["-o", "${fileDirname}/${fileBasenameNoExtension}", "${file}"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": ["$gcc"],
        "presentation": {
          "reveal": "always",
          "panel": "new"
        }
      }
    ]
  }
  